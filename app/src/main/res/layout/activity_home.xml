                        android:background="@color/gray"
                        android:visibility="gone" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constMore"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center"
                        android:weightSum="1">

                        <ImageView
                            android:id="@+id/ivMore"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="8dp"
                            android:layout_weight=".1"
                            android:src="@drawable/more"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint="@color/text_color" />

                        <TextView
                            android:id="@+id/tvMore"
                            style="@style/viewParent"
                            android:layout_marginStart="16dp"
                            android:text="@string/more"
                            android:textColor="@color/text_color"
                            android:textSize="@dimen/_12ssp"
                            app:layout_constraintBottom_toBottomOf="@+id/ivMore"
                            app:layout_constraintStart_toEndOf="@+id/ivMore"
                            app:layout_constraintTop_toTopOf="@+id/ivMore" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <View
                        android:id="@+id/view"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:background="@color/gray"
                        app:layout_constraintTop_toBottomOf="@+id/tvMore" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constSetting"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center"
                        android:weightSum="1">

                        <ImageView
                            android:id="@+id/ivSetting"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="8dp"
                            android:layout_weight=".1"
                            android:src="@drawable/ic_setting"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint="@color/text_color" />

                        <TextView
                            style="@style/viewParent"
                            android:layout_marginStart="16dp"
                            android:text="@string/settings"
                            android:textColor="@color/text_color"
                            android:textSize="@dimen/_12ssp"
                            app:layout_constraintBottom_toBottomOf="@+id/ivSetting"
                            app:layout_constraintStart_toEndOf="@+id/ivSetting"
                            app:layout_constraintTop_toTopOf="@+id/ivSetting" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:background="@color/gray" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="bottom"
                        android:orientation="vertical"
                        app:layout_constraintTop_toBottomOf="@+id/view">

                        <include
                            android:id="@+id/footer"
                            layout="@layout/custom_duo_view_footer" />

                    </LinearLayout>
                </LinearLayout>


            </ScrollView>
        </com.google.android.material.navigation.NavigationView>
    </androidx.drawerlayout.widget.DrawerLayout>
</layout>