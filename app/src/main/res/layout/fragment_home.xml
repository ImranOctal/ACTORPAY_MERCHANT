<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/parentFull"
                android:background="@drawable/top_round_drawble_white"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16sdp"
                    android:orientation="vertical"
                    app:layout_constraintTop_toTopOf="parent">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="2">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:layout_marginStart="@dimen/_12sdp"
                            android:layout_weight="1"
                            android:gravity="start">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvOrder"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivOrder"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_order_color"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvOrder"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="Orders"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivOrder" />

                                    <TextView
                                        android:id="@+id/tvTotalOrder"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:paddingVertical="@dimen/_5sdp"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvOrder" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvEarnMoney"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="120dp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivEarnMoneyColor"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_earn_money_color"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvEarnMoney"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="@string/earn_money"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivEarnMoneyColor" />

                                    <TextView
                                        android:id="@+id/tvPayrollManager"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvEarnMoney" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>
                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvProduct"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivProduct"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_manage_product"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvProduct"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="@string/product"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivProduct" />

                                    <TextView
                                        android:id="@+id/tvManageProduct"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvProduct" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvOutlet"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="120dp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivOutletColor"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_outlet_color"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvOutlet"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="@string/outlet"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivOutletColor" />

                                    <TextView
                                        android:id="@+id/tvTotalOutlet"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvOutlet" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>
                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvRole"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivRole"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_role_color"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvRole"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="@string/role"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivRole" />

                                    <TextView
                                        android:id="@+id/tvRoleTotal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:gravity="center"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvRole" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvSubMerchant"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivMerchant"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_merchant"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvMerchant"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="Sub Merchant"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivMerchant" />

                                    <TextView
                                        android:id="@+id/tvMangeMerchant"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:gravity="center"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvMerchant" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>
                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="2">


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvProfile"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivProfile"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_profile"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvProfile"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="@string/my_profile"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivProfile" />

                                    <TextView
                                        android:id="@+id/tvProfileManage"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvProfile" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cvReport"
                                android:layout_width="@dimen/_110sdp"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_10sdp"
                                android:elevation="@dimen/_15sdp"
                                android:shadowColor="@color/text_color"
                                app:cardCornerRadius="@dimen/_5ssp">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/ivReporting"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:src="@drawable/ic_reporting"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/tvReport"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/_6sdp"
                                        android:fontFamily="@font/poppins_bold"
                                        android:text="@string/reporting"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/ivReporting" />

                                    <TextView
                                        android:id="@+id/tvReportManager"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/poppins_regular"
                                        android:text="Manage"
                                        android:textColor="@color/text_color"
                                        android:textSize="@dimen/_12ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/tvReport" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>


                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>


                <TextView
                    android:id="@+id/tvBusinessName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="center"
                    android:text="Hi"
                    android:textSize="@dimen/_16sdp"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
